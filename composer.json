{
    "name": "heggsta/silverstripe-elemental-listing",
    "description": "A listing element for the silverstripe-elemental module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "listing",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Grant Heggie",
            "email": "grant@grantheggie.com"
        }
    ],
    "require": {
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/cms": "^4.3",
        "dnadesign/silverstripe-elemental": "^4",
        "symbiote/silverstripe-multivaluefield": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Heggsta\\ElementalListing\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}